{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "cartalyst/stripe": "^2.4",
        "certificationy/php-pack": "^1.4",
        "firebase/php-jwt": "^5.2",
        "graham-campbell/throttle": "^8.1",
        "illuminate/redis": "^7.25",
        "laravel/lumen-framework": "^7.0",
        "mattiasgeniar/php-percentages": "^1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-fractal": "^5.7",
        "stripe/stripe-php": "^7.47",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "lumen/database/seeds",
            "lumen/database/factories"
        ],
        "psr-4": {
            "App\\": "lumen/app/",
            "MyCerts\\": "src/MyCerts/",
            "MyCertsTests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
